                    <Grid x:Name="templateRoot" SnapsToDevicePixels="true">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="2"/>
                        </Grid.RowDefinitions>
                        <Border x:Name="mainBorder" BorderBrush="Transparent" Background="Transparent" Margin="0"/>
                        
                        <ContentPresenter x:Name="contentPresenter" ContentSource="Header" HorizontalAlignment="Center" Focusable="False" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{Binding VerticalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
                        <Grid VerticalAlignment="Bottom" Margin="7,0,7,3">
                            <Rectangle x:Name="light" Height="2" Fill="{TemplateBinding BorderBrush}" Opacity="0">
                                <Rectangle.BitmapEffect>
                                    <BlurBitmapEffect Radius="14"/>
                                </Rectangle.BitmapEffect>
                            </Rectangle>
                        </Grid>
                        <Grid Grid.Row="1" Margin="10,0,10,0">
                            <Rectangle x:Name="innerBorder" Height="2" RadiusX="1" RadiusY="1" Fill="{TemplateBinding BorderBrush}" Margin="0,2,0,0"/>
