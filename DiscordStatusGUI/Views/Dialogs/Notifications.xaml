<UserControl x:Class="DiscordStatusGUI.Views.Dialogs.Notifications"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DiscordStatusGUI.Views.Dialogs"
             xmlns:viewmodels="clr-namespace:DiscordStatusGUI.ViewModels.Dialogs"
             xmlns:converters="clr-namespace:DiscordStatusGUI.Converters"
             mc:Ignorable="d" 
             d:DesignHeight="450" Width="400">
    <UserControl.DataContext>
        <viewmodels:NotificationsViewModel/>
    </UserControl.DataContext>
    <Grid x:Name="notifications"/>
</UserControl>
